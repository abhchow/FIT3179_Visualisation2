{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/abhchow/FIT3179_Visualisation2/main/data/joined_pop_energy.csv"
    },
    "width":"container",
    "transform": [
      {
        "calculate": "datum.CoalTWh - datum.GasConsumptionTWh",
        "as": "temp_range"
      }
    ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "Year",
        "timeUnit": "year",
        "type": "nominal"
      },
      "y": {
        "aggregate": "count",
        "stack": "normalize"
      },
      "color": {
        "type": "nominal",
        "field": "weather",
        "scale": {
          "domain": [
            "sun",
            "fog",
            "drizzle",
            "rain",
            "snow"
          ],
          "range": [
            "#e7ba52",
            "#c7c7c7",
            "#aec7e8",
            "#1f77b4",
            "#9467bd"
          ]
        },
        "title": "Weather Type"        
      }
    },
    "config": {}
  }